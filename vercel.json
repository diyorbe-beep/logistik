{
  "version": 2,
  "functions": {
    "api/assets.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)\\.(js|mjs)",
      "dest": "/api/assets?asset=$1.$2"
    },
    {
      "src": "/assets/(.*)\\.(css)",
      "dest": "/api/assets?asset=$1.$2"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|mjs|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|xml|txt))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}